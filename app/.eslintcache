mberOfTasksLeft(setNumberOfCrossedOff(true));\n    } else {\n      changeLine('none');\n      setNumberOfTasksLeft(setNumberOfCrossedOff(false));\n    }\n  };\n\n  const setCrosses = () => {\n    if (item.completed) {\n      changeLine('line-through');\n    } else {\n      changeLine('none');\n    }\n  };\n\n  // Marks Tasks as Complete & Crosses Them Off\n  const completeTaskFunc = () => {\n    completeHandler();\n    crossOff();\n  };\n\n  let color = item['color'];\n\n  useEffect(() => {\n    setCrosses();\n  });\n\n  // setCrosses();\n  return (\n    <div className='toDoItem'>\n      <div className='thirtyThree'>\n        <div className='colorBar' style={{ backgroundColor: color }}></div>\n        <li style={{ textDecorationLine: isChecked }}>{item.name}</li>\n      </div>\n      <div className='thirtyThree'></div>\n      <div className='thirtyThree'>\n        <DeleteIcon onClick={deleteHandler} />\n        <input\n          type='checkbox'\n          className='checkButton'\n          onClick={completeTaskFunc}\n        />\n      </div>\n    </div>\n  );\n}\n","/Users/addyhebou/Local Documents/Apps/2021-Goals/app/src/Components/ShuffleCards.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/app/src/Components/Clipboard.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/app/src/Components/Step.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/app/src/Components/WelcomeFunc.js",["112","113"],"/Users/addyhebou/Local Documents/Apps/2021-Goals/app/src/Components/GoalArray.js",[],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","severity":1,"message":"119","line":9,"column":8,"nodeType":"120","messageId":"121","endLine":9,"endColumn":13},{"ruleId":"118","severity":1,"message":"122","line":10,"column":8,"nodeType":"120","messageId":"121","endLine":10,"endColumn":16},{"ruleId":"118","severity":1,"message":"123","line":1,"column":28,"nodeType":"120","messageId":"121","endLine":1,"endColumn":36},{"ruleId":"118","s