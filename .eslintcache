st';\nimport ShuffleTasks from './Pages/ShuffleTasks';\nimport React, { useState, useEffect, useRef } from 'react';\nimport RingLoader from 'react-spinners/RingLoader';\nimport LoadScreen from '../src/Components/LoadScreen';\nimport IndexFunc from './Pages/IndexFunc';\nimport GoalPageFunc from './Pages/GoalPageFunc';\nimport Routine from './Pages/Routine';\nimport alanBtn from '@alan-ai/alan-sdk-web';\nimport Productivity from './Pages/Productivity';\nimport NavbarIcons from './Components/NavbarIcons';\nimport MusicProjects from './Pages/MusicProjects';\n\nconst lib = require('./Services/getWeather');\n\nfunction App() {\n  useEffect(async () => {\n    setLoading(true);\n    setTimeout(() => {\n      setLoading(false);\n    }, 3000);\n    alanBtn({\n      key: 'df2d60f277a4c7147f9bdf25551d9a4d2e956eca572e1d8b807a3e2338fdd0dc/stage',\n      onCommand: async (commandData) => {\n        if (commandData.command === 'Good morning')\n          alert('Rise and shine pumpernickel');\n        else if (commandData.command === 'getWeather()')\n          console.log(await lib.weatherResponse());\n      },\n    });\n  }, []);\n\n  const [loading, setLoading] = useState(false);\n  return (\n    <div className=\"App\">\n      <alanBtn />\n      {loading ? (\n        <div>\n          <LoadScreen />\n          <RingLoader color={'#E584F6'} loading={loading} size={130} />\n        </div>\n      ) : (\n        <Router>\n          <Switch>\n            <Route exact path=\"/goal\" component={GoalPageFunc} />\n            <Route exact path=\"/weeklyList\" component={WeeklyList} />\n            <Route exact path=\"/shuffleTasks\" component={ShuffleTasks} />\n            <Route exact path=\"/routine\" component={Routine} />\n            <Route exact path=\"/productivity\" component={Productivity} />\n            <Route exact path=\"/projects\" component={MusicProjects} />\n            <Route component={IndexFunc} />\n            <Redirect to=\"/404\" />\n          </Switch>\n        </Router>\n      )}\n    </div>\n  );\n}\n\nexport default App;\n","/Users/addyhebou/Local Documents/Apps/2021-Goals/src/reportWebVitals.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/GoalPage.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/Index.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/WeeklyList.js",["147","148"],"import React, { Component, setState } from 'react';\nimport Form from '../Components/Form';\nimport ToDoList from '../Components/ToDoList';\nimport NavbarIcons from '../Components/NavbarIcons';\nimport '../Styles/App.scss';\n\nexport default class WeeklyList extends Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      inputText: '',\n      toDoList: JSON.parse(localStorage.getItem('lst')) || [],\n      length: 0,\n      color: '',\n      numberOfCrossedOff: 0,\n    };\n  }\n\n  async componentDidMount() {\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify({ title: 'React POST Request Example' }),\n    };\n    const url = 'https://ancient-coast-78698.herokuapp.com/';\n    const response = await fetch(url);\n    const data = await response.json();\n    data.forEach((element) => {\n      element['id'] = Math.random() * 1000;\n      if (element['category'] === 'Music Creator') element['color'] = '#EBBD87';\n      if (element['category'] === 'Financial Freedom')\n        element['color'] = '#C0EFAF';\n      else if (element['category'] === 'Performance')\n        element['color'] = '#F59393';\n      else if (element['category'] === 'Tech') element['color'] = '#AFD8EF';\n      else if (element['category'] === 'Personal') element['color'] = '#D1A9D4';\n    });\n    this.setState({\n      toDoList: data,\n    });\n  }\n\n  setColor = (hexCode) => {\n    this.setState({\n      color: hexCode,\n    });\n  };\n\n  setInputText = (text) => {\n    this.setState({\n      inputText: text,\n    });\n  };\n\n  setToDos = (lst) => {\n    // CURRENT PROBLEMS ARE HAPPENING RIGHT HERE\n    localStorage.setItem('lst', JSON.stringify(lst));\n    this.setState({\n      toDoList: JSON.parse(localStorage.getItem('lst')),\n      length: this.setNumberOfTasksLeft(this.state.numberOfCrossedOff),\n    });\n  };\n\n  setCategory = (category) => {\n    if (category === 'MusicCreator') {\n      return '#EBBD87';\n    } else if (category === 'FinancialFreedom') return '#C0EFAF';\n    else if (category === 'Performance') return '#F59393';\n    else if (category === 'MusicTech') return '#AFD8EF';\n    else if (category === 'Personal') return '#D1A9D4';\n    return '#AFD8EF';\n  };\n\n  setNumberOfCrossedOff = (bool) => {\n    if (bool) {\n      this.setState({\n        numberOfCrossedOff: this.state.numberOfCrossedOff + 1,\n      });\n    } else {\n      this.setState({\n        numberOfCrossedOff: this.state.numberOfCrossedOff - 1,\n      });\n    }\n    return this.state.numberOfCrossedOff;\n  };\n\n  setNumberOfTasksLeft = (numberOfCrossedOff) => {\n    return JSON.parse(localStorage.getItem('lst')).length - numberOfCrossedOff;\n  };\n\n  render() {\n    return (\n      <div>\n        <NavbarIcons />\n        <h1>Your Weekly Goals</h1>\n        <h3>Total Tasks: {this.state.length}</h3>\n        <Form\n          inputText={this.state.inputText}\n          setInputText={this.setInputText}\n          toDos={this.state.toDoList}\n          setToDos={this.setToDos}\n          setCategory={this.setCategory}\n        />\n        <ToDoList\n          item={this.state.inputText}\n          toDos={this.state.toDoList.length !== 0 ? this.state.toDoList : []}\n          setToDos={this.setToDos}\n          categoryColor={this.state.color}\n          setNumberOfCrossedOff={this.setNumberOfCrossedOff}\n          setNumberOfTasksLeft={this.setNumberOfTasksLeft}\n        />\n      </div>\n    );\n  }\n}\n","/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/ShuffleTasks.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/IndexFunc.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/Routine.js",["149","150","151","152"],"import React from 'react';\nimport { useState, useEffect } from 'react';\nimport '../Styles/Routine.scss';\nimport NavbarIcons from '../Components/NavbarIcons';\nconst lib = require('../Services/getRoutine');\n\nexport default function Routine() {\n  const [isDay, setIsDay] = useState(true);\n  const routine = isDay ? lib.getRoutine('morning') : lib.getRoutine('evening');\n  let [num, setNum] = useState(0);\n  const [steps, setSteps] = useState([]);\n  const [highlighted, setHighlighted] = useState(routine[num]);\n\n  useEffect(() => {\n    setHighlighted(routine[num]);\n  });\n\n  return (\n    <div>\n      <NavbarIcons />\n      <h1>Routine</h1>\n      <div className=\"main\">\n        <div>\n          <caption>Where image will be</caption>\n          <h2>\n            <strong>{highlighted}</strong>\n          </h2>\n        </div>\n        <div>\n          <ul className=\"routineContainer\">\n            {routine.map((step, index) => {\n              return highlighted === step ? (\n                <li\n                  className=\"routineStep\"\n                  style={{ transform: 'scale(1.25)', margin: '2vh 0vw' }}\n                >\n                  {step}\n                </li>\n              ) : (\n                <li\n                  className=\"routineStep\"\n                  onClick={() => {\n                    setNum(index);\n                  }}\n                >\n                  {step}\n                </li>\n              );\n            })}\n          </ul>\n          <button\n            onClick={() => {\n              if (num > 0) setNum(num - 1);\n            }}\n          >\n            Go back\n          </button>\n          <button\n            onClick={() => {\n              if (num < routine.length - 1) setNum(num + 1);\n            }}\n          >\n            Go forward\n          </button>\n        </div>\n      </div>\n    </div>\n  );\n}\n","/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/GoalPageFunc.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/Productivity.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Pages/MusicProjects.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/LoadScreen.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/NavbarIcons.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Services/getWeather.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/Step.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/Form.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/ToDoList.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/GoalSet.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/Welcome.js",["153","154"],"import React from 'react';\nimport { useState, useEffect } from 'react';\nimport '../Styles/WelcomeFunc.scss';\nimport ToDoPreview from './ToDoPreview';\nconst lib = require('../Services/getWeather');\n\nexport default function WelcomeFunc() {\n  useEffect(async () => {\n    try {\n      const vals = await lib.getWeather();\n      setCity(vals[0]);\n      setRegion(vals[1]);\n      setTemp(vals[2]);\n      setCondition(vals[3]);\n      setIcon(vals[4]);\n      setFeel(vals[5]);\n      setTimeOfDay(vals[6]);\n      setMessage(vals[7]);\n    } catch (e) {\n      console.log(e);\n    }\n  }, []);\n\n  const [temp, setTemp] = useState(null);\n  const [feel, setFeel] = useState(null);\n  const [condition, setCondition] = useState(null);\n  const [icon, setIcon] = useState(null);\n  const [timeOfDay, setTimeOfDay] = useState('day');\n  const [city, setCity] = useState('');\n  const [region, setRegion] = useState('');\n  const [message, setMessage] = useState('');\n\n  return (\n    <div className=\"welcome\">\n      <h1>Good {timeOfDay}, Addy.</h1>\n      {temp ? (\n        <div>\n          <p>\n            The weather in {city}, {region} is currently {temp}º, but it feels\n            like {feel}º.\n          </p>\n          <p> The condition is {condition}. </p>\n          <p>{message}</p>\n          <img src={icon} />\n          <p>Here are the items for you to do today:</p>\n          <ToDoPreview />\n        </div>\n      ) : (\n        <p>\n          No information on the weather & your to-do list right now... might\n          wanna check your Internet...\n        </p>\n      )}\n    </div>\n  );\n}\n","/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Services/getRoutine.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/StepFunc.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/GoalArray.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Constants/musicProjects.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Constants/goalTable.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/Trello Board/List.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/ToDo.js",["155"],"import React, { useEffect, useState } from 'react';\nimport DeleteIcon from '@material-ui/icons/Delete';\n\nexport default function ToDo({\n  item,\n  toDos,\n  setToDos,\n  setNumberOfCrossedOff,\n  setNumberOfTasksLeft,\n}) {\n  // Handles deleting items\n  const deleteHandler = () => {\n    if (item.completed === true)\n      setNumberOfTasksLeft(setNumberOfCrossedOff(false));\n    let updatedList = toDos.filter((el) => el.id !== item.id);\n    setToDos(updatedList);\n  };\n\n  // Handles completion states\n  const completeHandler = () => {\n    let updatedList = toDos.forEach((iter) => {\n      if (iter.id === item.id) {\n        item.completed = !item.completed;\n      }\n    });\n  };\n\n  // Handles the Strikethrough Texts\n  const [isChecked, changeLine] = useState('none');\n  const crossOff = () => {\n    if (isChecked === 'none') {\n      changeLine('line-through');\n      setNumberOfTasksLeft(setNumberOfCrossedOff(true));\n    } else {\n      changeLine('none');\n      setNumberOfTasksLeft(setNumberOfCrossedOff(false));\n    }\n  };\n\n  const setCrosses = () => {\n    if (item.completed) {\n      changeLine('line-through');\n    } else {\n      changeLine('none');\n    }\n  };\n\n  // Marks Tasks as Complete & Crosses Them Off\n  const completeTaskFunc = () => {\n    completeHandler();\n    crossOff();\n  };\n\n  let color = item['color'];\n\n  useEffect(() => {\n    setCrosses();\n  });\n\n  // setCrosses();\n  return (\n    <div className='toDoItem'>\n      <div className='thirtyThree'>\n        <div className='colorBar' style={{ backgroundColor: color }}></div>\n        <li style={{ textDecorationLine: isChecked }}>{item.name}</li>\n      </div>\n      <div className='thirtyThree'></div>\n      <div className='thirtyThree'>\n        <DeleteIcon onClick={deleteHandler} />\n        <input\n          type='checkbox'\n          className='checkButton'\n          onClick={completeTaskFunc}\n        />\n      </div>\n    </div>\n  );\n}\n","/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Constants/categories.js",[],"/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/ToDoPreview.js",["156","157"],"import React from 'react';\nimport { useEffect, useState } from 'react';\n\nexport default function ToDoPreview() {\n  useEffect(async () => {\n    const url = 'https://ancient-coast-78698.herokuapp.com/';\n    const response = await fetch(url);\n    if (!response.ok) throw new Error('HTTP Error');\n    const res = await response.json();\n    setTasks(res);\n  }, []);\n  const [tasks, setTasks] = useState([]);\n  return (\n    <ul>\n      {tasks.map((task) => {\n        if (task['completed'] === false) return <li>{task['name']}</li>;\n      })}\n    </ul>\n  );\n}\n","/Users/addyhebou/Local Documents/Apps/2021-Goals/src/Components/GoalSet